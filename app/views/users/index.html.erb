<section class="home"> 
  <h1>Welcome to Social Trainer</h1>
    <%= form_for :user, url: sessions_path do |f| %> 
    <p>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </p> 
    <p>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </p>
    <p>
      <%= f.submit %>
    </p>
    <% end %>
<section>

